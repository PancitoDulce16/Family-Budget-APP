rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: These rules are insecure but are the only ones that allow the app to function.
    // They allow any authenticated user to read and write all data.
    // This is a workaround for a very unusual and persistent permissions issue.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}